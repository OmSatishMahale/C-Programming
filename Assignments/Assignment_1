
---------------------Que 1)---------------------
// Program to Divide two numbers

#include <stdio.h>

int Divide(int iNo1,int iNo2)
{
    int iAns = 0;
    iAns = iNo1 / iNo2;
    
    return iAns;
}

int main()
{

    int iValue1 = 15,iValue2 = 5;
    int iRet =0;

    iRet = Divide(iValue1,iValue2);
    printf("Division of Two Numbers is : %d\n",iRet);

    return 0;
}


---------------------Que 2)---------------------
//Program to print 5 times Marvellous

#include<stdio.h>

void Display()
{
    int i = 0;
    for(i=1;i<=5;i++)
    {
        printf("Marvellous\n");
    }
}

int main()
{

    Display();
    
    return 0;
}


---------------------Que 3)---------------------
//To Display Number from 5 to 1


#include<stdio.h>

void Display()
{
    int i = 0;
    i = 5;

    while(i >= 1)
    {
        printf("%d\n",i);
        i--;
    }
}

int main()
{
    Display();
    return 0;
}


---------------------Que 4)---------------------
//Check whether number is divisible by 5 or not

#include<stdio.h>
#include<stdbool.h>

bool Check(int iNo)
{
    if ((iNo % 5) == 0)
    {
        return true;
    }
    else
    {
        return false;
    }
}

int main()
{

    int iValue = 0;
    bool bRet = false;

    printf("Enter Number : \n");
    scanf("%d",&iValue);

    bRet = Check(iValue);

    if(bRet == true)
    {
        printf("Number is divisible by 5\n");
    }
    else
    {
        printf("Number is not divisible by 5\n");
    }

    return 0;
}


---------------------Que 5)---------------------
//To Display * number of times user enter
#include<stdio.h>

void Display(int iNo)
{
    int iCnt = 0;
    for(iCnt =1;iCnt<=iNo;iCnt++)
    {
        printf("*\n");
    }
}

int main()
{

    int iValue = 0;

    printf("Enter The Number of * to Display : \n");
    scanf("%d",&iValue);

    Display(iValue);

    return 0;
}
